{
  "name": "scssketch",
  "version": "0.1.0",
  "engines": {
    "sketch": ">=3.0"
  },
  "skpm": {
    "name": "scssketch",
    "manifest": "src/manifest.json",
    "main": "scssketch.sketchplugin",
    "assets": [
      "assets/**/*"
    ]
  },
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch --run",
    "postinstall": "npm run build && skpm-link"
  },
  "devDependencies": {
    "@skpm/builder": "^0.5.2"
  },
  "author": "Max Oppenheimer <max.oppenheimer@devmynd.com> and Sarah Aslanifar <sarah.aslanifar@devmynd.com>",
  "dependencies": {}
}
